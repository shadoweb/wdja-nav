<?xml version="1.0" encoding="utf-8"?>
<xml mode="wdja" author="wdja">
  <configure>
    <node>item</node>
    <field>name,tpl_default</field>
    <base>item_list</base>
  </configure>
  <item_list>
    <item>
      <name><![CDATA[gallery]]></name>
      <tpl_default><![CDATA[
      <li><img src="{$url}" id="gallery_{$order}"/></li>
      ]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[valcode]]></name>
      <tpl_default><![CDATA[<img src="{$=#global_images_route}code/code.php" onclick="this.src='{$=#global_images_route}code/code.php?' + Math.floor(Math.random() * 99999 + 1)" align="absmiddle" style="cursor:hand;cursor:pointer;vertical-align: middle;" />]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[client_alert]]></name>
      <tpl_default><![CDATA[<html>
<head>
<meta http-equiv="content-type" content="text/html; charset={$=CHARSET}">
<title>Alert</title>
</head>
<body>
<script type="text/javascript">
alert("{$alert}");
{$dispose};
</script>
</body>
</html>
]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[client_redirect]]></name>
      <tpl_default><![CDATA[<html>
<head>
<meta http-equiv="content-type" content="text/html; charset={$=CHARSET}">
<title>Redirect</title>
</head>
<body>
<script type="text/javascript">
location.href = "{$url}";
</script>
</body>
</html>
]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[em]]></name>
      <tpl_default><![CDATA[<script type="text/javascript">
function insertstr(newcode) {
  itextner("{$content}", newcode);
}
</script>
<a href="javascript:insertstr('[em]1[/em]')"><img src="{$=#global_images_route}em/1.gif" border="0"></a> <a href="javascript:insertstr('[em]2[/em]')"><img src="{$=#global_images_route}em/2.gif" border="0"></a> <a href="javascript:insertstr('[em]3[/em]')"><img src="{$=#global_images_route}em/3.gif" border="0"></a> <a href="javascript:insertstr('[em]4[/em]')"><img src="{$=#global_images_route}em/4.gif" border="0"></a> <a href="javascript:insertstr('[em]5[/em]')"><img src="{$=#global_images_route}em/5.gif" border="0"></a> <a href="javascript:insertstr('[em]6[/em]')"><img src="{$=#global_images_route}em/6.gif" border="0"></a> <a href="javascript:insertstr('[em]7[/em]')"><img src="{$=#global_images_route}em/7.gif" border="0"></a> <a href="javascript:insertstr('[em]8[/em]')"><img src="{$=#global_images_route}em/8.gif" border="0"></a> <a href="javascript:insertstr('[em]9[/em]')"><img src="{$=#global_images_route}em/9.gif" border="0"></a> <a href="javascript:insertstr('[em]10[/em]')"><img src="{$=#global_images_route}em/10.gif" border="0"></a> <a href="javascript:insertstr('[em]11[/em]')"><img src="{$=#global_images_route}em/11.gif" border="0"></a> <a href="javascript:insertstr('[em]12[/em]')"><img src="{$=#global_images_route}em/12.gif" border="0"></a> <a href="javascript:insertstr('[em]13[/em]')"><img src="{$=#global_images_route}em/13.gif" border="0"></a> <a href="javascript:insertstr('[em]14[/em]')"><img src="{$=#global_images_route}em/14.gif" border="0"></a> <a href="javascript:insertstr('[em]15[/em]')"><img src="{$=#global_images_route}em/15.gif" border="0"></a> <a href="javascript:insertstr('[em]16[/em]')"><img src="{$=#global_images_route}em/16.gif" border="0"></a>
]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[cutepage]]></name>
      <tpl_default><![CDATA[<script type="text/javascript">
function cc_cutepage(strers)
{
  var tstrers = get_num(strers) - 1;
  if (tstrers < 0) tstrers = 0;
  return tstrers * {$pagesize};
}
</script>
{@firstpage}<span class="ash">{$=ii_itake('global.lng_cutepage.firstpage', 'lng')}</span>{|}<a href="{$URLfirst}">{$=ii_itake('global.lng_cutepage.firstpage', 'lng')}</a>{@firstpage} {@prepage}<span class="ash">{$=ii_itake('global.lng_cutepage.prepage', 'lng')}</span>{|}<a href="{$URLpre}">{$=ii_itake('global.lng_cutepage.prepage', 'lng')}</a>{@prepage} {@nextpage}<span class="ash">{$=ii_itake('global.lng_cutepage.nextpage', 'lng')}</span>{|}<a href="{$URLnext}">{$=ii_itake('global.lng_cutepage.nextpage', 'lng')}</a>{@nextpage} {@lastpage}<span class="ash">{$=ii_itake('global.lng_cutepage.lastpage', 'lng')}</span>{|}<a href="{$URLlast}">{$=ii_itake('global.lng_cutepage.lastpage', 'lng')}</a>{@lastpage} {$=ii_itake('global.lng_cutepage.chpage', 'lng')}<input type="text" id="go-page-num" class="i1" value="{$xpagenum}" maxlength="5" onkeyup="this.value=get_num(this.value);if (this.value>{$pagenums}) this.value={$pagenums};" style="width: 42px;text-align: center;vertical-align: middle;"> <input type="button" value="GO" onclick="location_href('{$goURL}');" class="button"> {$npagenum}/{$pagenums} {$pagesize}{$=ii_itake('global.lng_cutepage.perpage', 'lng')}]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[web_message]]></name>
      <tpl_default><![CDATA[<script type="text/javascript">
var tstr = '{$backurl}';
if(tstr == '') tstr = -1;
if(get_num(tstr) != 0) setTimeout('history.go(tstr);', 2500);
else setTimeout(function(){window.location.href=tstr;}, 2500);
</script>
<div id="middle">
  <div id="middleContent">
    <p style="width:100%;margin:0 auto;height:200px;line-height:200px;text-align:center;">{$message}</p>
  </div>
</div>]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[web_messages]]></name>
      <tpl_default><![CDATA[<script type="text/javascript">
setTimeout('history.go(-1);', 2500);
</script>
<div id="middle">
  <div id="middleContent">
    <p style="width:100%;margin:0 auto;height:200px;line-height:200px;text-align:center;">{$message}</p>
  </div>
</div>
]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[web_switch]]></name>
      <tpl_default><![CDATA[<!doctype html>
<html>
<head>
<meta charset="utf-8">
<title>{$message}</title>
<style>
html,body,div,h1,*{margin:0;padding:0;}
body{
	background-color:#fefefe;
	color:#333
}
.box{
	width:580px;
	margin:0 auto;
}
h1{
	font-size:20px;
	text-align:center; 
	background:url(data:image/png;base64,) no-repeat top center; 
	padding-top:160px;
	margin-top:30%;
	font-weight:normal;
}

</style>
</head>
<body>
<div class="box">
<h1>{$message}</h1>
</div>
</body>
</html>
]]></tpl_default>
    </item>
    <item>
      <name><![CDATA[echo_error]]></name>
      <tpl_default><![CDATA[<script type="text/javascript">
alert('{$message}');
</script>
]]></tpl_default>
    </item>
<item>
      <name><![CDATA[pagenum]]></name>
      <tpl_default><![CDATA[
            <style type="text/css">
                #cutepage {
                    text-align: center;
                }
                #cutepage a {
                    display: inline-block;
                    width: 30px;
                    height: 30px;
                    line-height: 30px;
                    font-size: 14px;
                    font-family: Tahoma;
                    background: #F8F8F8;
                    margin: 0 5px;
                    border: 1px solid #DDD;
                    border-radius: 4px;
                }
                #cutepage a.np-page {width: 70px}
                #cutepage a.current, #cutepage a:hover {
                    background: #DDD;
                    text-decoration: none;
                }
            </style>
            {$pre}{@}<a{$current} href="{$pageurl}">{$pagenum}</a>{@}{$next}
        ]]></tpl_default>
    </item>
  </item_list>
</xml>